 <div style="width:600px; height:500px; margin-left:150px;background-color:#D2B48C; border: 3px solid #191970; color:black">
 <div class="row"> 
   <div class="col-md-8"> 
     <%= render "users/project_name"%>
     <h2 style="margin-left:40px;">Sign In</h2>
     <%= form_for :session, url: sessions_path do |f|  %>
       <div class="form-group">
         <%= f.label :email, style:'margin-left:40px;' %>
         <%= f.email_field :email, style:'margin-left:40px;', autofocus: true, class: 'form-control', placeholder: "Enter email" %>
       </div>
       <div class="form-group">
         <%= f.label :password, style:'margin-left:40px;' %>
         <%= f.password_field :password, style:'margin-left:40px;', class: 'form-control', placeholder: "Enter password" %>
       </div>
       <%= link_to "Forgotten password?", new_password_reset_path, style:'margin-left:40px;color:blue;'%>
       <div class="form-group", style="margin-left:40px;">
          <%= f.label :remember_me, class: 'checkbox' do %>
            Remember_me <%= f.check_box :remember_me, style:'margin-left:10px;'%>
          <% end %>
         <%= f.submit "Sign in", class: 'btn btn-primary' %>
       </div>
     <% end %>
   </div>
 </div>
</div>